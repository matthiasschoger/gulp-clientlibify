<?xml version="1.0" encoding="utf-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment>FileVault Package Properties</comment>
    <entry key="createdBy"><%= deployUsername %></entry>
    <entry key="name"><%= packageName %></entry>
    <entry key="lastModified"><%= isoDateTime %></entry>
    <entry key="lastModifiedBy"><%= deployUsername %></entry>
    <entry key="created"><%= isoDateTime %></entry>
    <entry key="buildCount">1</entry>
    <entry key="version"><%= packageVersion %></entry>
    <entry key="dependencies"/>
    <entry key="packageFormatVersion">2</entry>
    <entry key="group"><%= packageGroup %></entry>
    <entry key="lastWrapped"><%= isoDateTime %></entry>
    <entry key="description"><%= packageDescription %></entry>
    <entry key="lastWrappedBy"><%= deployUsername %></entry>
</properties>